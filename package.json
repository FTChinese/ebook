{
  "private": true,
  "engines": {
    "node": ">=0.12.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.2.1",
    "del": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-babel": "^6.1.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^0.13.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.0",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^3.0.0",
    "main-bower-files": "^2.5.0",
    "wiredep": "^2.2.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "quotes": [
        0,
        "single"
      ],
      "strict": [
        0
      ],
      "no-trailing-spaces": [0, { "skipBlankLines": true }],
      "no-unused-vars": [0, { "vars": "all", "args": "after-used" }],
      "space-infix-ops": [0, {"int32Hint": false}],
      "comma-spacing": [0, { "before": true, "after": false }],
      "key-spacing": [0, {"beforeColon": false, "afterColon": false}],
      "comma-dangle": [0, "always-multiline"],
      "no-mixed-spaces-and-tabs": [0],
      "no-multi-spaces": [0],
      "no-spaced-func": [0],
      "semi-spacing": [0],
      "no-empty": [0],
      "space-unary-ops": [0],
      "no-shadow": [0],
      "no-underscore-dangle": [0],
      "yoda": [0],
      "eol-last": [0]
    },
    "globals": {
      "FastClick": true,
      "FTScroller": true,
      "escape": true,
      "Swipe": true,
      "_scrollerOn": true,
      "FTColumnflow": true,
      "ga": true,
      "fa": true,
      "playFunction": true,
      "_clicktoTurnPage": true,
      "console": true,
      "playslide": true,
      "changepage": true,
      "organizepage": true,
      "wheelObject": true,
      "gFTScrollerActive": true,
      "openRail": true,
      "closeRail": true,
      "switchRail": true
    }
  }
}
